{
    "name": "tmrw-life/ntak-guru-php-sdk",
    "description": "PHP SDK for NTAK.guru API",
    "keywords": [
        "tmrw",
        "tmrw-life",
        "ntak",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/TMRW-Life/ntak.guru-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Levente Otta",
            "email": "levente.otta@tmrw.life",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "archtechx/enums": "^1.0.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.9",
        "phpunit/phpunit": "^10.5",
        "spatie/ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "TmrwLife\\NtakGuru\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmrwLife\\NtakGuru\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {

            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
