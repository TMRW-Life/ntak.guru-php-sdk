{
    "name": "tmrw-life/ntak-guru-php-sdk",
    "description": "This is my package ntak-guru-php-sdk",
    "keywords": [
        "TMRW-Life",
        "ntak-guru-php-sdk"
    ],
    "homepage": "https://github.com/tmrw-life/ntak-guru-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Levente Otta",
            "email": "levente.otta@tmrw.life",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "phpunit/phpunit": "^10.1",
        "spatie/ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "TmrwLife\\NtakGuru\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmrwLife\\NtakGuru\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {

            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
